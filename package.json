    "build": "npm run docs:build"
  },
  "dependencies": {
    "foreach-cli": "^1.8.1",
    "markdown-it-fontawesome": "^0.3.0",
    "prismjs": "^1.17.1",
    "vue-tabs-component": "^1.5.0",
    "vuepress": "1.6.0",
    "vuepress-theme-craftdocs": "^1.3.6"
  }
}
